{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "prefer-const": "error",
    "no-var": "error",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "@next/next/no-img-element": "warn",
    "jsx-a11y/alt-text": "warn"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
